{
  "kind" : "Deployment",
  "apiVersion" : "apps/v1",
  "metadata" : {
    "name" : "consumer",
    "namespace" : "default"
  },
  "spec" : {
    "replicas" : 1,
    "selector" : {
      "matchLabels" : {
        "app" : "consumer"
      }
    },
    "template" : {
      "metadata" : {
        "name" : "consumer",
        "labels" : {
          "app" : "consumer",
          "metrics" : "prometheus"
        }
      },
      "spec" : {
        "containers" : [ {
          "name" : "consumer",
          "image" : "docker.io/consumer:0.0.1",
          "imagePullPolicy" : "IfNotPresent",
          "ports" : [ {
            "containerPort" : 8999,
            "name" : "backend-port",
            "protocol" : "TCP"
          } ],
          "env" : [ {
            "name" : "LOG_LEVEL",
            "value" : "INFO"
          }, {
            "name" : "SK8S_RUN_MODE",
            "valueFrom" : {
              "configMapKeyRef" : {
                "key" : "sk8s-run-mode",
                "name" : "sk8s-config"
              }
            }
          }, {
            "name" : "HOST_IP",
            "valueFrom" : {
              "fieldRef" : {
                "fieldPath" : "status.hostIP",
                "apiVersion" : "v1"
              }
            }
          }, {
            "name" : "POD_IP",
            "valueFrom" : {
              "fieldRef" : {
                "fieldPath" : "status.podIP",
                "apiVersion" : "v1"
              }
            }
          }, {
            "name" : "JAVA_OPTS",
            "value" : "-server -Dpidfile.path=/dev/null -Djava.io.tmpdir=/opt/docker -Dnetworkaddress.cache.ttl=20 -XX:MaxRAMPercentage=75 "
          }, {
            "name" : "BACKEND_SERVER",
            "value" : "true"
          }, {
            "name" : "LOG_LEVEL",
            "value" : "INFO"
          }, {
            "name" : "SK8S_RUN_MODE",
            "valueFrom" : {
              "configMapKeyRef" : {
                "key" : "sk8s-run-mode",
                "name" : "sk8s-config"
              }
            }
          }, {
            "name" : "HOST_IP",
            "valueFrom" : {
              "fieldRef" : {
                "fieldPath" : "status.hostIP",
                "apiVersion" : "v1"
              }
            }
          }, {
            "name" : "POD_IP",
            "valueFrom" : {
              "fieldRef" : {
                "fieldPath" : "status.podIP",
                "apiVersion" : "v1"
              }
            }
          }, {
            "name" : "JAVA_OPTS",
            "value" : "-server -Dpidfile.path=/dev/null -Djava.io.tmpdir=/opt/docker -Dnetworkaddress.cache.ttl=20 -XX:MaxRAMPercentage=75 "
          } ],
          "resources" : {
            "limits" : {
              "cpu" : "3"
            },
            "requests" : {
              "cpu" : "0.2",
              "memory" : "512m"
            }
          },
          "livenessProbe" : {
            "httpGet" : {
              "port" : 8999,
              "path" : "/health",
              "scheme" : "HTTP"
            },
            "initialDelaySeconds" : 30,
            "timeoutSeconds" : 2,
            "periodSeconds" : 5,
            "successThreshold" : 1,
            "failureThreshold" : 5
          },
          "readinessProbe" : {
            "httpGet" : {
              "port" : 8999,
              "path" : "/health",
              "scheme" : "HTTP"
            },
            "initialDelaySeconds" : 10,
            "timeoutSeconds" : 2,
            "periodSeconds" : 5,
            "successThreshold" : 1,
            "failureThreshold" : 5
          }
        } ],
        "automountServiceAccountToken" : false,
        "restartPolicy" : "Always",
        "dnsPolicy" : "ClusterFirst",
        "imagePullSecrets" : [ {
          "name" : "docker-hub-credentials"
        } ]
      }
    }
  }
}
